<%= form_for resource, :as => resource_name, :url => registration_path(resource_name) do |f| %>
<ul>
	<li><%= f.label :name, "Type your name" %><%= f.text_field :name %></li>
	
  <li><%= f.label :email, "Type your email address" %><%= f.email_field :email %></li>

  <li><%= f.label :password, "Choose a password" %><%= f.password_field :password %></li>

  <li><%= f.label :password_confirmation, "Confirm your password" %><%= f.password_field :password_confirmation %></li>

  <li><%= f.submit "Create my profile" %></li>
</ul>
<% end %>